<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    <link rel="stylesheet" src="//cdn.datatables.net/1.10.13/css/jquery.dataTables.min.css"/>
    @Scripts.Render("~/bundles/modernizr")
    <script src="//cdn.datatables.net/1.10.13/js/jquery.dataTables.min.js"></script>

</head>
<body>
    <nav class="navbar fixed-top navbar-dark bg-inverse">

        <!-- Brand -->
        <a class="navbar-brand" href="~/Home/Index"><img src="~/Content/Images/HH-Logo-black.png" class="d-inline-block align-top" width="35" height="35"/>
        Product Catalog
        </a>

        <!-- Links -->
        
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        
        <footer>
            <p>&copy; @DateTime.Now.Year - Henrik Häggbom</p>
        </footer>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
